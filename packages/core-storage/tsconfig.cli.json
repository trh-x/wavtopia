{
  "compilerOptions": {
    "composite": true,
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["prisma/seed.ts", "prisma/seeds/**/*.ts"],
  "exclude": ["node_modules", "dist", "types"]
}
